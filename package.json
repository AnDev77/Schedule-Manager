  "bugs": {
    "url": "https://github.com/AnDev77/Schedule-Manager/issues"
  },
  "homepage": "https://github.com/AnDev77/Schedule-Manager#readme",
  "devDependencies": {
    "concurrently": "^9.0.1"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http-status-codes": "^2.3.0",
    "node-cron": "^3.0.3",
    "nodecron": "^0.2.0",
    "nodemailer": "^6.9.15",
    "schedule-manager": "file:"
  }
}
